{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "interface-name": [false],
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [false],
    "object-literal-sort-keys": false,
    "no-namespace": [false],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ]
  }
}
